{
  "name": "Catch me if you can",
  "version": "1.0",
  "description": "This extension is to have fun with google homepage",
  "manifest_version": 2,

  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "css": ["./buttonstyle.css"],
      "js": ["./main.js"]
    }
  ],

  "web_accessible_resources": ["./buttonStyle.css"]
}